<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Default -->
    <CornerRadius x:Key="BorderDefaultCornerRadius">0</CornerRadius>
    <Thickness x:Key="BorderDefaultBorderThickness">1</Thickness>
    <Thickness x:Key="BorderDefaultMargin">4</Thickness>
    <Thickness x:Key="BorderDefaultPadding">0</Thickness>

    <!-- Overlay -->
    <CornerRadius x:Key="OverlayBorderDefaultCornerRadius">0</CornerRadius>
    <Thickness x:Key="OverlayBorderDefaultBorderThickness">0</Thickness>
    <Thickness x:Key="OverlayBorderDefaultMargin">0</Thickness>
    <Thickness x:Key="OverlayBorderDefaultPadding">0</Thickness>
</ResourceDictionary>