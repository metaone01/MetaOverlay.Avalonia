    <SolidColorBrush x:Key="ComboBoxItemPressedForeground" Color="#0040FF" />
    <SolidColorBrush x:Key="ComboBoxItemSelectedForeground" Color="#0040FF" />

    <BoxShadows x:Key="ComboBoxPopupBoxShadow">0 0 1 #4A000000, 0 4 14 #1A000000</BoxShadows>
