<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="ComboBoxSelectorDisabledBackground" Color="#80A9A9A9" />
    <SolidColorBrush x:Key="ComboBoxSelectorDisabledBorderBrush" Color="#80696969" />
    <SolidColorBrush x:Key="ComboBoxSelectorBorderBrush" Color="#0080FF" />
    <SolidColorBrush x:Key="ComboBoxSelectorPointeroverBorderBrush" Color="#0040FF" />
    <SolidColorBrush x:Key="ComboBoxSelectorFocusBorderBrush" Color="#0040FF" />
    <SolidColorBrush x:Key="ComboBoxSelectorPressedBorderBrush" Color="#0040FF" />

    <SolidColorBrush x:Key="ComboBoxIconDefaultForeground" Color="#0080FF" />
    <SolidColorBrush x:Key="ComboBoxIconPointeroverForeground" Color="#0040FF" />
