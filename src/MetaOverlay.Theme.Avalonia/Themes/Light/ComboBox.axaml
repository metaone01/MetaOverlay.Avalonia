    <SolidColorBrush x:Key="ComboBoxIconFocusForeground" Color="#0040FF" />
    <SolidColorBrush x:Key="ComboBoxIconDisabledForeground" Color="#696969" />

    <SolidColorBrush x:Key="ComboBoxDisabledForeground" Color="#696969" />


    <SolidColorBrush x:Key="ComboBoxPopupBackground" Color="#CCFFFFFF" />
    <SolidColorBrush x:Key="ComboBoxPopupBorderBrush" Color="#0080FF" />

    <SolidColorBrush x:Key="ComboBoxItemForeground" Color="Black" />
